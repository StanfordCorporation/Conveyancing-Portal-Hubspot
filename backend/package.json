{
  "name": "conveyancing-portal-backend",
  "version": "1.0.0",
  "description": "Serverless backend for Conveyancing Portal - HubSpot Integration",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "build": "npx playwright install chromium-headless-shell",
    "postinstall": "npx playwright install chromium-headless-shell",
    "deploy": "vercel --prod",
    "lint": "eslint ."
