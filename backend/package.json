  "keywords": [
    "conveyancing",
    "hubspot",
    "serverless",
    "vercel"
  ],
  "dependencies": {
    "@hubspot/api-client": "^10.1.0",
    "@vercel/kv": "^3.0.0",
    "axios": "^1.6.2",
    "axios-retry": "^4.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "docusign-esign": "^8.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.9",
    "playwright": "^1.56.1",
    "redis": "^5.9.0",
    "speakeasy": "^2.0.0",
    "stripe": "^19.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "vercel": "^33.7.1"
  }
}
